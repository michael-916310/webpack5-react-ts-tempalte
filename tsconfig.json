{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
